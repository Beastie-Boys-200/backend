{
  "dialog_id": 3,
  "messages": [
    { "id": 1, "role": "user", "text": "Здравствуйте, мне нужна помощь с картой." },
    { "id": 2, "role": "assistant", "text": "Здравствуйте! В чём проблема?" },
    { "id": 3, "role": "user", "text": "Платёж в магазине отклонили, хотя деньги есть." },
    { "id": 4, "role": "assistant", "text": "Сейчас проверю. Укажите последние 4 цифры карты." },
    { "id": 5, "role": "user", "text": "5521." },
    { "id": 6, "role": "assistant", "text": "Спасибо. Платёж отклонён по причине подозрительной активности." },
    { "id": 7, "role": "user", "text": "Почему подозрительной?" },
    { "id": 8, "role": "assistant", "text": "Платёж сделан в другой стране." },
    { "id": 9, "role": "user", "text": "Да, я в Турции сейчас." },
    { "id": 10, "role": "assistant", "text": "Понимаю. У вас включена геолокационная защита." },
    { "id": 11, "role": "user", "text": "Можно её отключить?" },
    { "id": 12, "role": "assistant", "text": "Да, но только временно — до 24 часов." },
    { "id": 13, "role": "user", "text": "Отключите, пожалуйста." },
    { "id": 14, "role": "assistant", "text": "Готово, попробуйте оплатить снова." },
    { "id": 15, "role": "user", "text": "Сработало, спасибо." },
    { "id": 16, "role": "assistant", "text": "Рада помочь." },
    { "id": 17, "role": "user", "text": "А можно отключить защиту на всё время поездки?" },
    { "id": 18, "role": "assistant", "text": "Да, если вы подтвердите поездку." },
    { "id": 19, "role": "user", "text": "Как подтвердить?" },
    { "id": 20, "role": "assistant", "text": "Я отправлю вам push-код." },
    { "id": 21, "role": "assistant", "text": "Отправила." },
    { "id": 22, "role": "user", "text": "Код 5278." },
    { "id": 23, "role": "assistant", "text": "Спасибо, защита отключена на 14 дней." },
    { "id": 24, "role": "user", "text": "Хорошо." },
    { "id": 25, "role": "assistant", "text": "Нужно что-то ещё?" },
    { "id": 26, "role": "user", "text": "Да, хочу поднять лимит на снятие." },
    { "id": 27, "role": "assistant", "text": "На какую сумму?" },
    { "id": 28, "role": "user", "text": "До 100 000 ₽." },
    { "id": 29, "role": "assistant", "text": "Сейчас проверю возможность…" },
    { "id": 30, "role": "assistant", "text": "Лимит можно поднять до 80 000 ₽." },
    { "id": 31, "role": "user", "text": "Хорошо, сделайте." },
    { "id": 32, "role": "assistant", "text": "Лимит повышен." },
    { "id": 33, "role": "user", "text": "Спасибо." },
    { "id": 34, "role": "assistant", "text": "Могу ещё чем-то помочь?" },
    { "id": 35, "role": "user", "text": "У меня есть вопрос по комиссии в банкоматах." },
    { "id": 36, "role": "assistant", "text": "Комиссия зависит от банка-оператора. У нас комиссия 0%." },
    { "id": 37, "role": "user", "text": "То есть если банкомат чужой, будет комиссия?" },
    { "id": 38, "role": "assistant", "text": "Да, но иногда банкоматы не берут комиссию — зависит от сети." },
    { "id": 39, "role": "user", "text": "Понял." },
    { "id": 40, "role": "assistant", "text": "Могу подсказать ближайшие банкоматы без комиссии." },
    { "id": 41, "role": "user", "text": "Да, подскажите." },
    { "id": 42, "role": "assistant", "text": "Я нашла два — отправила карту вам в приложение." },
    { "id": 43, "role": "user", "text": "Вижу, спасибо." },
    { "id": 44, "role": "assistant", "text": "Хорошего дня!" },
    { "id": 45, "role": "user", "text": "Подождите, ещё вопрос." },
    { "id": 46, "role": "assistant", "text": "Слушаю." },
    { "id": 47, "role": "user", "text": "Как узнать, когда лимит снова уменьшится?" },
    { "id": 48, "role": "assistant", "text": "Я могу включить уведомление за сутки до изменения." },
    { "id": 49, "role": "user", "text": "Да, включите." },
    { "id": 50, "role": "assistant", "text": "Готово." },
    { "id": 51, "role": "user", "text": "Теперь всё, спасибо." },
    { "id": 52, "role": "assistant", "text": "Рада была помочь!" },
    { "id": 53, "role": "user", "text": "До свидания." },
    { "id": 54, "role": "assistant", "text": "До связи!" },
    { "id": 55, "role": "user", "text": "Хорошего дня!" },
    { "id": 56, "role": "assistant", "text": "И вам!" },
    { "id": 57, "role": "user", "text": "Спасибо." }
  ]
}
